// Place your settings in this file to overwrite the default settings
{
    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "vs-nomo-dark",
    "workbench.sideBar.location": "left",
    // Editor
    "editor.fontFamily": "'JetbrainsMono Nerd Font','Source Code Pro', Monaco, 'Courier New', monospace",
    "editor.fontSize": 18,
    "editor.rulers": [
        80,
        100,
        120
    ],
    "editor.wordWrapColumn": 120,
    "editor.minimap.enabled": false,
    // Files
    "files.trimTrailingWhitespace": false,
    "files.trimFinalNewlines": false,
    // Diff editor
    "diffEditor.ignoreTrimWhitespace": false,
    // Customizes which terminal application to run on OS X.
    "terminal.external.osxExec": "iTerm.app",
    // Integrated terminal
    "terminal.integrated.fontSize": 15,
    // Custom file associations
    "files.associations": {
        "zshenv": "shellscript",
        "zprofile": "shellscript",
        "zshrc": "shellscript",
        "zlogin": "shellscript",
        "zlogout": "shellscript",
        "zpreztorc": "shellscript"
    },
    "[git-commit]": {
        "editor.rulers": [
            72,
            80
        ]
    },
    "git.autofetch": true,
    "git.inputValidationLength": 80,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "groovyLint.insight.enable": false,
    "groovyLint.lint.trigger": "onSave",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.semanticHighlighting.enabled": true,
    "java.configuration.checkProjectSettingsExclusions": false,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "java.project.importHint": false,
    "redhat.telemetry.enabled": false,
    "terminal.integrated.tabs.enabled": true,
    "shellcheck.customArgs": [
        "-x"
    ],
    "editor.copyWithSyntaxHighlighting": false,
    "mypy.runUsingActiveInterpreter": true,
    "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
    "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm:ss",
    "gitlens.defaultTimeFormat": "HH:mm",
    "gitlens.codeLens.enabled": false,
    "gitlens.defaultDateLocale": null,
    "cSpell.language": "en,fr",
    "cSpell.userWords": [
        "avro",
        "DataProc",
        "distdataproc",
        "gcloud",
        "Hadoop",
        "hefr",
        "l'UI",
        "notebooks",
        "packag√©",
        "spark",
        "Spark",
        "unbundling"
    ],
    "python.formatting.provider": "black"
}